{
  "name": "ununu-ink",
  "description": "Leveraging digital music collaboration with Git-based data formats",
  "version": "1.0.0",
  "private": true,
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "lint": "eslint",
