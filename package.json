  "lint-staged": {
    "*.{js,json}": [
      "prettier",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true
  }
